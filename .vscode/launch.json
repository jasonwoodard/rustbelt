{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Ann Arbor: SolveDay (tsx)",
      "type": "node",
      "request": "launch",
      "runtimeExecutable": "${workspaceFolder}/node_modules/.bin/tsx",
      "program": "${workspaceFolder}/src/index.ts",
      "args": [
        "--trip", "${workspaceFolder}/data/day-Mon-AA-v1-2025-09-15-T2136.json", 
        "--day", "Mon",
        "--mph", "35",
        "--default-dwell", "30",
        "--lambda", "0.10",
        "--seed", "7",
        "--verbose",
        "--out", "${workspaceFolder}/out/day-Fri-RX-v2-2025-09-18-T0215.json",
        "--kml", "${workspaceFolder}/out/itinerary-${runId}-${timestamp}.kml",
        "--csv", "${workspaceFolder}/out/itinerary-${runId}-${timestamp}.csv",
        "--html", "${workspaceFolder}/out/itinerary-${runId}-${timestamp}.html"
      ],
      "cwd": "${workspaceFolder}",
      "console": "integratedTerminal",
      "internalConsoleOptions": "openOnFirstSessionStart",
      "skipFiles": ["<node_internals>/**", "${workspaceFolder}/node_modules/**"],
      "sourceMaps": true,
      "smartStep": true,
      "stopOnEntry": false,
      "autoAttachChildProcesses": true
    },
